{
    "name": "tonyle/cakephp-jwt-auth",
    "type": "cakephp-plugin",
    "description": "CakePHP plugin for authenticating using JSON Web Tokens",
    "keywords":[
        "cakephp",
        "authenticate",
        "authentication",
        "jwt"
    ],
    "homepage": "http://github.com/tonyle/cakephp-jwt-auth",
    "authors": [
        {
            "name":"Tonyle",
            "role":"Author",
            "homepage":"https://github.com/tonyle"
        }
    ],
    "license": "MIT",
    "support": {
        "source":"https://github.com/tonyle/cakephp-jwt-auth",
        "issues":"https://github.com/tonyle/cakephp-jwt-auth/issues"
    },
    "require": {
        "cakephp/cakephp": "~3.0",
        "firebase/php-jwt": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JwtAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JwtAuth\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
